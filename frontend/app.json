{
    "expo": {
        "name": "AFC Estate",
        "slug": "frontend",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "scheme": "frontend",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "deepLinking": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
            "edgeToEdgeEnabled": true,
            "package": "com.safecities.permaculture"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/logo.png",
            "name": "AFC Estate",
            "meta": {
                "viewport": "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"
            }
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/sc-splash.jpeg",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "3d20f1f9-675d-4bc0-9587-bda2fecbc774"
            }
        }
    }
}
